<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_mr_account_move_form_inherited" model="ir.ui.view">
        <field name="name">view_mr_account_move_form_inherited</field>
        <field name="model">account.move</field>
        <field name="inherit_id" ref="account.view_move_form"/>
        <field name="arch" type="xml">
            <xpath expr="//button[@name='button_cancel']" position="replace">
                <field name="commission_applied_id" invisible="1"/>
                <button name="commission_test" string="Probar comision" type="object"/>
            </xpath>
        </field>
    </record>
</odoo>
